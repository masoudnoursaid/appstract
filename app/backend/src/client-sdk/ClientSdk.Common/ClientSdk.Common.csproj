<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <RootNamespace>Appstract.Common.Sdk</RootNamespace>
        <AssemblyName>Appstract.Common.Sdk</AssemblyName>
        <BaseOutputPath>..\nuget</BaseOutputPath>
        <GenerateDocumentationFile>False</GenerateDocumentationFile>
        <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
        <PackageId>Appstract.Common.Sdk</PackageId>
        <Title>Appstract Common Sdk</Title>
        <Authors>Mason Chase</Authors>
        <Description>Appstract Common Sdk</Description>
        <PackageTags>Appstract Common Sdk</PackageTags>
        <Version>1.0.0</Version>
    </PropertyGroup>


    <ItemGroup>
        <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    </ItemGroup>
</Project>
