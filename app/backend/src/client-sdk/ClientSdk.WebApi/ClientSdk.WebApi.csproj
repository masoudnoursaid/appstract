<Project Sdk="Microsoft.NET.Sdk">


  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>Appstract.Web.Sdk</RootNamespace>
    <AssemblyName>Appstract.Web.Sdk</AssemblyName>
    <BaseOutputPath>..\nuget</BaseOutputPath>
    <GenerateDocumentationFile>False</GenerateDocumentationFile>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>Appstract.Web.Sdk</PackageId>
    <Title>Appstract Web Sdk</Title>
    <Authors>Mason Chase</Authors>
    <Description>Appstract Web Sdk</Description>
    <PackageTags>Appstract web Sdk</PackageTags>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>
</Project>
